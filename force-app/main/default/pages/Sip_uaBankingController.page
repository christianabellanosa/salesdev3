                var today = new Date();
                var start = new Date();
                var end = new Date();

                start.setFullYear(today.getFullYear(), today.getMonth() - 1, today.getDate());
                end.setFullYear(today.getFullYear(), today.getMonth(), today.getDate());

                return {
